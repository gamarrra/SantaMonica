@model List<OrdenDto>
<!DOCTYPE html>
<html lang="en">

<head>
    <title>CSS Template</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>
    <div class="content">
        <table class="table">
            <tr>
                <th>Marca</th>
                <th>Producto</th>
                <th>Costo</th>
                <th>Cantidad</th>
                <th>Zona</th>
            </tr>

            @foreach (var orden in Model)
            {
                <tr>
                    <td>@orden.Marca</td>
                    <td>@orden.ProduNombre</td>
                    <td>@orden.ProduCosto</td>
                    <td>@orden.Cantidad</td>
                    <td>@orden.ZonaNombre</td>
                </tr>
            }
        </table>
                @Html.ActionLink("Vaciar", "Vaciar")
                @Html.ActionLink("Confirmar", "Confirmar")
</body>

</html>